<view class="page">
  <view class="page__bd">
        <view class="weui-panel weui-panel_access">
            <view class="weui-panel__bd" wx:for="{{recycleList}}" wx:key="unique" id="{{index}}">
                <view class="weui-media-box weui-media-box_appmsg" style="margin:-15px;">
                      <view class="weui-media-box__hd">
                          <image class="weui-media-box__thumb" src="{{imageUrlPre+item.path}}" />
                      </view>
                      <view class="weui-media-box__bd">
                          <h4 class="weui-media-box__title">{{item.label}} <text decode='true'>&nbsp;&nbsp;&nbsp;{{item.price}}</text></h4>
                      </view>
                  </view>
            </view>
        </view>
  </view>
</view>